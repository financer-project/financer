{
    "name": "financer",
    "version": "0.1.0",
    "scripts": {
        "dev": "blitz dev",
        "build": "dotenv -e .env.production yarn db:generate && blitz build",
        "build:docker:latest": "docker build -t financer:latest .",
        "start": "blitz start",
        "studio": "prisma studio",
        "lint": "eslint .",
        "test": "vitest run --passWithNoTests --coverage",
        "test:watch": "vitest",
        "db:generate": "prisma generate",
        "db:migrate:deploy": "prisma migrate deploy",
        "db:push": "prisma db push",
        "test:start:app": "dotenv -e .env.test blitz start",
        "test:start:app:dev": "dotenv -e .env.test blitz dev",
        "test:e2e": "dotenv -e .env.test start-server-and-test test:start:app 3000 cypress:e2e",
        "test:e2e:headed": "dotenv -e .env.test start-server-and-test test:start:app 3000 cypress:e2e:headed",
        "test:e2e:open": "dotenv -e .env.test start-server-and-test test:start:app:dev 3000 cypress:e2e:open",
        "test:component": "dotenv -e .env.test yarn cypress:component",
        "test:component:headed": "dotenv -e .env.test yarn cypress:component:headed",
        "test:component:open": "dotenv -e .env.test yarn cypress:component:open",
        "cypress:e2e": "cypress run --e2e --browser chrome",
        "cypress:e2e:headed": "cypress run --e2e --browser chrome --headed",
        "cypress:e2e:open": "cypress open --e2e",
        "cypress:component": "cypress run --component --browser chrome",
        "cypress:component:headed": "cypress run --component --browser chrome --headed",
        "cypress:component:open": "cypress open --component",
        "coverage": "nyc report --reporter=lcov && coveralls < coverage/lcov.info"
    },
    "prisma": {
        "schema": "src/lib/db/schema/"
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix"
        ]
    },
    "dependencies": {
        "@blitzjs/auth": "^2.2.1",
        "@blitzjs/next": "^2.2.1",
        "@blitzjs/rpc": "^2.2.1",
        "@prisma/client": "^6.4.1",
        "@radix-ui/react-alert-dialog": "^1.1.5",
        "@radix-ui/react-avatar": "^1.1.2",
        "@radix-ui/react-dialog": "^1.1.5",
        "@radix-ui/react-dropdown-menu": "^2.1.5",
        "@radix-ui/react-label": "^2.1.1",
        "@radix-ui/react-popover": "^1.1.5",
        "@radix-ui/react-scroll-area": "^1.2.2",
        "@radix-ui/react-select": "^2.1.5",
        "@radix-ui/react-separator": "^1.1.1",
        "@radix-ui/react-slot": "1.1.1",
        "@radix-ui/react-toast": "1.2.5",
        "@radix-ui/react-tooltip": "^1.1.7",
        "@tailwindcss/postcss": "4.0.1",
        "autoprefixer": "10.4.20",
        "blitz": "^2.2.1",
        "class-variance-authority": "0.7.1",
        "clsx": "^2.1.1",
        "cmdk": "1.0.0",
        "currency-codes": "2.2.0",
        "formik": "^2.4.6",
        "lucide-react": "0.475.0",
        "luxon": "3.5.0",
        "next": "^15.1.7",
        "postcss": "^8.5.1",
        "prisma": "^6.5.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "recharts": "^2.15.1",
        "secure-password": "^4.0.0",
        "tailwind-merge": "^3.0.2",
        "tailwindcss": "^4.0.1",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.24.1",
        "zod-formik-adapter": "1.3.0"
    },
    "devDependencies": {
        "@cypress/code-coverage": "3.13.12",
        "@next/env": "^15.1.6",
        "@testcontainers/mysql": "^10.18.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "@types/luxon": "3.4.2",
        "@types/node": "^22.12.0",
        "@types/preview-email": "^3.1.0",
        "@types/react": "^19.0.8",
        "@types/react-dom": "19.0.3",
        "@typescript-eslint/eslint-plugin": "^8.22.0",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitest/coverage-istanbul": "^3.0.8",
        "ajv": "^8.17.1",
        "babel-plugin-istanbul": "7.0.0",
        "cypress": "^14.1.0",
        "dotenv-cli": "8.0.0",
        "eslint": "^9.19.0",
        "eslint-config-next": "^15.1.6",
        "eslint-config-prettier": "^10.0.1",
        "istanbul-lib-coverage": "3.2.2",
        "jsdom": "^26.0.0",
        "lcov-result-merger": "5.0.1",
        "lint-staged": "^15.4.3",
        "preview-email": "^3.1.0",
        "prisma-mock": "^0.10.3",
        "start-server-and-test": "2.0.10",
        "testcontainers": "^10.18.0",
        "typescript": "^5.7.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.0.8",
        "vitest-mock-extended": "^3.0.1"
    }
}
